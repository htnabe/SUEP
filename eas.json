// update channel number everytime the app version increases
{
  "cli": {
    "version": ">= 0.59.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "releaseChannel": "dev-1.1.6.18"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "releaseChannel": "prev-1.1.6.18"
    },
    "production": {
      "releaseChannel": "prod-1.1.6.18"
    }
  },
  "submit": {
    "production": {
      "ios": {},
      "android": {
        "track": "production",
        "releaseStatus": "draft"
      }
    }
  }
}
